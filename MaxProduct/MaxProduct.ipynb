   "source": [
    "def max_prod(a:list):\n",
    "    a = sorted(a)\n",
    "    print(a)\n",
    "    tail = a[0] * a[1] * a[-1]\n",
    "    print(tail)\n",
    "    head = a[-1] * a[-2] * a[-3]\n",
    "    print(head)\n",
    "    return max(tail, head)"
   ]
